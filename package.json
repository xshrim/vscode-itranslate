{
  "name": "itranslate",
  "displayName": "itranslate",
  "description": "vscode translate",
  "version": "1.1.0",
  "publisher": "xshrim",
  "icon": "images/icon.png",
  "author": {
    "name": "xshrim",
    "email": "xshrim@gmail.com"
  },
  "engines": {
    "vscode": "^1.2.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "repository": "https://github.com/xshrim/vscode-itranslate.git",
  "contributes": {
    "commands": [
      {
        "command": "iTranslate.select",
        "title": "选择翻译服务",
        "category": "iTranslate"
      },
      {
        "command": "iTranslate.selectReplace",
        "title": "替换翻译内容",
        "category": "iTranslate"
      },
      {
        "command": "iTranslate.selectReplaceFormat",
        "title": "替换成驼峰格式",
        "category": "iTranslate"
      }
    ],
    "keybindings": [
      {
        "command": "iTranslate.selectReplace",
        "key": "ctrl+alt+r",
        "mac": "ctrl+cmd+r"
      },
      {
        "command": "iTranslate.selectReplaceFormat",
        "key": "ctrl+alt+shift+r",
        "mac": "ctrl+cmd+shift+r"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "xshrim translate",
      "properties": {
        "iTranslate.proxy": {
          "type": "string",
          "default": null,
          "markdownDescription": "在线翻译代理"
        },
        "iTranslate.proxyList": {
          "type": "array",
          "default": [
            "google"
          ],
          "markdownDescription": "在线翻译代理白名单"
        },
        "iTranslate.hover": {
          "type": "boolean",
          "default": false,
          "description": "开启悬停取词"
        },
        "iTranslate.noOriginal": {
          "type": "boolean",
          "default": true,
          "description": "不显示原文"
        },
        "iTranslate.verbose": {
          "type": "boolean",
          "default": false,
          "description": "显示详细翻译结果"
        },
        "iTranslate.localLang": {
          "type": "string",
          "default": "zh-CN",
          "description": "本地语言简称"
        },
        "iTranslate.threshold": {
          "type": "integer",
          "default": 0,
          "description": "长句优先在线翻译"
        },
        "iTranslate.provider": {
          "type": "string",
          "default": "google",
          "enum": [
            "google",
            "microsoft",
            "baidu",
            "youdao",
            "tencent"
          ],
          "enumDescriptions": [
            "谷歌",
            "微软",
            "百度",
            "有道",
            "腾讯"
          ],
          "description": "在线翻译提供商"
        },
        "iTranslate.priority": {
          "type": "string",
          "default": "offlineFirst",
          "enum": [
            "offlineFirst",
            "onlineFirst",
            "onlyOffline",
            "onlyOnline"
          ],
          "enumDescriptions": [
            "离线优先",
            "在线优先",
            "仅用离线",
            "仅用在线"
          ],
          "description": "翻译方式优先级"
        }
      }
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.2.0",
    "eslint": "^5.13.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "typescript": "^3.3.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "got": "^11.8.5",
    "hpagent": "^0.1.2",
    "nodejieba": "^2.5.2",
    "tough-cookie": "^3.0.0",
    "vscode-languageclient": "^5.2.1",
    "vscode-languageserver": "^5.2.1"
  }
}
